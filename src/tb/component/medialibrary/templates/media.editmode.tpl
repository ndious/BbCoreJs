<li data-uid={{uid}} class="bb5-selector-item">
    <p><a title="{{title}}" href="javascript:;"><img alt="{{title}}" src="{{image}}"></a></p>
    <p><a title="{{title}}" href="javascript:;">{{title}}</a></p>
    <p>
        <button class="btn btn-simple btn-xs show-media-btn"><i class="fa fa-eye"></i> See</button>
        <button class="btn btn-simple btn-xs edit-media-btn"><i class="fa fa-pencil"></i> Edit</button>
        <button class="btn btn-simple btn-xs addandclose-btn"><i class="fa fa-plus"></i> Select and close</button>
        <button class="btn btn-simple btn-xs del-media-btn"><i class="fa fa-trash-o"></i> Delete</button>
    </p>
</li>
